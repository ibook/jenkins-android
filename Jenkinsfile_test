pipeline {
  	agent any
    stages {
        stage('Environment Example') {
            steps {
              script{
              	def envs = sh(returnStdout: true, script: 'env').split('\n')
				envs.each { line  ->
					String[] tmp = line.split(',');
    				println "$tmp[0] = $tmp[1]"
				}	
              }
            }
        }
    }
}	